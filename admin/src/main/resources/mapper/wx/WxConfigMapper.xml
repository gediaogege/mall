<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mall.mapper.wx.WxConfigMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mall.entity.wx.WxConfig">
        <id column="id" property="id" />
        <result column="shop_id" property="shopId" />
        <result column="app_id" property="appId" />
        <result column="app_secret" property="appSecret" />
        <result column="web_url" property="webUrl" />
        <result column="server_url" property="serverUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, shop_id AS shopId, app_id AS appId, app_secret AS appSecret, web_url AS webUrl, server_url AS serverUrl
    </sql>

</mapper>
